    // Services
    section#services.content-section.bg-light.text-black.text-center
      .container
        .content-section-heading
          h3.text-secondary.mb-0 Choose your own finish
          h2.mb-5 Now More Personal
        .row
          .col-lg-4.col-md-6.mb-5.mb-lg-0
